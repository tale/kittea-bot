{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"esModuleInterop": true,
		"moduleResolution": "node",
		"outDir": "./dist",
		"rootDir": "./src",
		"incremental": true,
		"strict": true,
		"removeComments": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": "./src",
		"skipLibCheck": true
	}
}
